<?xml version='1.0' encoding='utf-8'?>
<widget id="com.stockmobilesoftware.catchcrabs" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Catch Crabs</name>
    <description>
        Click the crab!
    </description>
    <author email="matthewstockinger88@gmail.com" href="http://catch-crabs.com">
        Matthew Stockinger
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon src="res/ic_app_icon2.xml" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
